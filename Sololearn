#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n;
    
    for ( ; n>0; n--) {
        cout <<n<< endl;
        if (n%5==0) {
            cout << "Beep\n";
        }
    }
    
    return 0;
}

#include <iostream>
using namespace std;

int main() {
    double items[] = {500, 12.4, 94, 45, 3, 81, 1000.9, 85, 90, 1, 35};
    
    int percent;
    for (int x=0; x<11; x++) {
        cin >> percent;
        cout << items[x]-items[x]*percent/100 << " ";
    }
    
    return 0;
}

#include <iostream>
using namespace std;

int main() {
    int n;
    cin>>n; //size of the array
    int nums = 0;
    int max = nums;
    for(int i=0; i<n; i++) {
        cin >> nums;
        if(nums > max)
            max = nums;
    }
    cout << max;

    return 0;
}

#include <iostream>
using namespace std;

int main() {
    int ages[5];

    for (int i = 0; i < 5; ++i) {
        cin >> ages[i];
    }
    float youngest = ages[0];
    for (int a = 0; a < 5; a++) {
        if(youngest>ages[a]){
            youngest = ages[a];
        }
    }
    
    float price = 50 - (50*youngest/100);
    cout << price;
    
    return 0;
}

#include <iostream>
using namespace std;

int max(int num1, int num2) {
    int high;
    if (num1 > num2) {
        high = num1;
    }
    else {
        high = num2;
    }
    return high;
}

int main() {
    int first;
    cin >> first;
    int second;
    cin >> second;

    cout << max(first, second);
    
    return 0;
}

#include <iostream>
#include <cstdlib>
using namespace std;


int main() {
    srand(0);
    int range;
    cin >> range;
    int PIN[4];
    for (int x=0; x<4; x++) {
        PIN[x] = 1+(rand() % range);
    }
    cout <<PIN[0] <<PIN[1] <<PIN[2] <<PIN[3] <<endl;
    return 0;
}

#include <iostream>
#include <string>
using namespace std;

void printOrder(string myorder = "Black tea") {
    cout << myorder << endl;
}

int main() {
    string friendOrder;
    cin >> friendOrder;
    printOrder();
    printOrder(friendOrder);
    
    return 0;
}

#include <iostream>
#include <string>
using namespace std;

void add(int x, int y) {
    cout << x + y << endl;
}
void add(double x, double y) {
    cout << x + y << endl;
}

int main() {
    add(5,6);
    add(1.2, 6.5);
    
    return 0;
}

#include <iostream>
#include <string>
using namespace std;

void winners(string customers[], int size, int n) {
    for (int i = (n-1); i < size; i+=n) {
        cout << customers[i] << endl;
    }
}

int main() {

    string customers[] = {"Alice", "Bob", "Rayan", "Emma", "Ann", "Bruce", "Synthia", "Daniel", "Richard", "Sam", "Nick", "Mary", "Paul"};
    int n;
    cin >> n;
    winners(customers, 13, n);
    
    return 0;
}

#include <iostream>
using namespace std;

void promotion(int *megabytes) {
    
    int multiplier;
    cin >> multiplier;
    *megabytes = *megabytes * multiplier;
}

int main() {
    int megabytes;
    cin >> megabytes;
    cout << "Before the promotion: " << megabytes << endl;
    promotion(&megabytes);
    
    cout << "After the promotion: " << megabytes << endl;
    
    return 0;
}

#include <iostream>
using namespace std;
int reversnumbers(int y){
    int remainder = 0;
    int reverse = 0;
    while (y > 0) {
        remainder = y % 10;
        reverse = reverse *10 + remainder;
        y/=10;
    }
    return reverse;
}

bool isPalindrome(int x) {
    if(x==reversnumbers(x)){
        return true;
    }
    else{
        return false;
    }   
}

int main() {
    int n;
    cin >> n;
    
    if(isPalindrome(n)) {
        cout < <n <<" is a palindrome";
    }
    else {
        cout << n <<" is NOT a palindrome";
    }
    return 0;
}
